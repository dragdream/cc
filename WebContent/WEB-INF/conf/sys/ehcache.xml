<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" monitoring="autodetect" dynamicConfig="false">
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="1000" eternal="false" timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" diskSpoolBufferSizeMB="10" />
	<!-- 永久存储数据 -->
	<cache name="eternalData" maxElementsInMemory="1000" eternal="true" overflowToDisk="false" memoryStoreEvictionPolicy="FIFO" />
	
	<cache name="sectionData" maxElementsInMemory="5000" eternal="false" timeToIdleSeconds="3600" timeToLiveSeconds="3600" overflowToDisk="false" memoryStoreEvictionPolicy="FIFO" />
	
	
</ehcache>
